<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Seekbar">
        <attr name="progressDrawable" format="reference"  />
        <attr name="thumb" format="reference"  />
    </declare-styleable>
    
    <attr name="fromDegrees" format="float|reference" />
    <attr name="toDegrees" format="float|reference" />
    <attr name="childSize" format="dimension|reference" />

	<declare-styleable name="ArcLayout">
        <attr name="fromDegrees"/>
        <attr name="toDegrees"/>
        <attr name="childSize"/>
	</declare-styleable>

    <declare-styleable name="ArcMenu">
        <attr name="fromDegrees"/>
        <attr name="toDegrees"/>
        <attr name="childSize"/>
    </declare-styleable>
</resources>
